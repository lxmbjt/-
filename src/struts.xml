<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<package name="struts2" extends="struts-default">
	<action name="Login" class="struts2.action.LoginAction" method="execute">
		<result name="success">/main.jsp</result>
		<result name="input">/login.jsp</result>
	</action>
	<action name="Register" class="struts2.action.RegisterAction" method="execute">
		<result name="success">/login.jsp</result>
		<result name="input">/register.jsp</result>
	</action>
	<action name="Logout" class="struts2.action.LogoutAction" method="execute">
		<result name="success">/main.jsp</result>
		
	</action>
	<action name="Search" class="struts2.action.SearchAction" method="execute">
		<result name="success">/search.jsp</result>
		
	</action>
	<action name="House" class="struts2.action.HouseAction" method="execute">
		<result name="success">/house_new.jsp</result>
		
	</action>
	<action name="SearchByTag" class="struts2.action.SearchByTagAction" method="execute">
	<result name="success">/search.jsp</result>
		
	</action>
	<action name="AddFavorite" class="struts2.action.AddFavoriteAction" method="execute">
	<result name="success">/house_new.jsp</result>
	</action>
	<action name="Report" class="struts2.action.ReportAction" method="execute">
	<result name="success">/accuse_success.jsp</result>
	<result name="fail">/accusation.jsp</result>
	</action>

</package>
</struts>    
